{
  "name": "type-support",
  "version": "1.0.0",
  "description": "Type Support Try",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build && node lib/run.greeter.js",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbhrockzz/type-support.git"
  },
  "keywords": [
    "Type",
    "Support"
  ],
  "author": "DBH",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbhrockzz/type-support/issues"
  },
  "homepage": "https://github.com/dbhrockzz/type-support#readme",
  "devDependencies": {
    "@types/jest": "^25.1.5",
    "@types/node": "^13.11.0",
    "jest": "^25.2.6",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
